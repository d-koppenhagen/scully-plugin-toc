{
  "name": "scully-plugin-toc",
  "version": "0.0.6",
  "description": "this plugin for scully provides a postRenderer to generate a table of contents for the rendered route content",
  "license": "MIT",
  "repository": "https://github.com/d-koppenhagen/scully-plugin-toc",
  "author": "Danny Koppenhagen <mail@d-koppenhagen.de> (https://d-koppenhagen.de)",
  "homepage": "https://github.com/d-koppenhagen/scully-plugin-toc",
  "keywords": [
    "scully",
    "toc",
    "postRenderer",
    "angular",
    "ssr",
    "ssg",
    "JAMStack"
  ],
  "bugs": {
    "url": "https://github.com/d-koppenhagen/scully-plugin-toc/issues",
    "email": "mail@d-koppenhagen.de"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "build:watch": "npm run build -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "release": "standard-version"
  },
  "dependencies": {
    "@scullyio/scully": "^0.0.78",
    "jsdom": "^16.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "@types/puppeteer": "^2.0.1",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
